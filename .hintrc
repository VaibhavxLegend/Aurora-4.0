{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "disown-opener": "off",
    "axe/text-alternatives": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[referrerpolicy=no-referrer-when-downgrade]",
          "iframe[loading]"
        ]
      }
    ],
    "apple-touch-icons": "off",
    "meta-viewport": "off",
    "manifest-file-extension": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-moz-column-width"
        ]
      }
    ]
  }
}